{
  "extends": [
    "react-app",
    "google",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 120
      }
    ],
    "require-jsdoc": 0,
    "no-invalid-this": "off",
    "linebreak-style": "off"
  }
}